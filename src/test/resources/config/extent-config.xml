<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!-- report theme -->
        <!-- standard, dark -->
        <theme>dark</theme>

        <!-- document encoding -->
        <!-- defaults to UTF-8 -->
        <encoding>UTF-8</encoding>

        <!-- protocol for script and stylesheets -->
        <!-- defaults to https -->
        <protocol>https</protocol>

        <!-- title of the document -->
        <documentTitle>BDD Cucumber Extent Report</documentTitle>

        <!-- report name - displayed at top-nav -->
        <reportName>BDD Test Report</reportName>

        <!-- timestamp format -->
        <!-- for example, yyyy-MM-dd HH:mm:ss a -->
        <timeStampFormat>yyyy-MM-dd HH:mm:ss a</timeStampFormat>

        <!-- custom javascript -->
        <scripts>
            <![CDATA[
                $(document).ready(function() {

                });
            ]]>
        </scripts>

        <!-- custom styles -->
        <styles>
            <![CDATA[

            ]]>
        </styles>
    </configuration>
</extentreports>
